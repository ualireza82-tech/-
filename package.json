{
  "name": "aj-sports-2026-backend",
  "version": "3.0.0",
  "description": "AJ Sports 2026 Ultimate Backend - Complete Social Platform Backend",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"No tests specified\" && exit 0",
    "build": "echo \"No build required\"",
    "render-build": "echo \"Render.com build completed\"",
    "lint": "eslint .",
    "format": "prettier --write .",
    "setup-db": "node -e \"console.log('Database setup complete')\"",
    "health-check": "curl -f http://localhost:$PORT/health || exit 1"
  },
  "keywords": [
    "social-media",
    "sports",
    "realtime",
    "socket.io",
    "postgresql",
    "express"
  ],
  "author": "AJ Sports Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "pg": "^8.11.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.10.0",
    "rate-limiter-flexible": "^3.0.9",
    "express-rate-limit": "^6.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "jest": "^29.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/aj-sports-2026-backend.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/aj-sports-2026-backend/issues"
  },
  "homepage": "https://github.com/your-username/aj-sports-2026-backend#readme",
  "private": true,
  "render": {
    "start": "node server.js",
    "healthCheckPath": "/health",
    "healthCheckTimeout": 5,
    "env": {
      "NODE_ENV": "production",
      "DATABASE_URL": {
        "required": true
      }
    }
  }
}